<h1>New Entry</h1>

<form action="/entries" method="entry">
  <p>
    <label for="title">Title</label>
    <input type="text" name="title">
  </p>
  <p>
    <label for="description">Description</label>
    <input type="text" name="description">
  </p>
  <p>
    <label for="occured_on">Occurred On</label>
    <input type="date" name="occured_on">
  </p>
  <button>Save</button>
</form>

